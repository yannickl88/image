{
    "name": "yannickl88/image",
    "description": "Library for reading, transforming and writing image files.",
    "authors": [
        {
            "name": "Yannick de Lange",
            "email": "yannick.l.88@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "ext-gd": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "sybio/gif-creator": "^1.0",
        "sybio/gif-frame-extractor": "^1.0",
        "symfony/filesystem": "^4.3",
        "vimeo/psalm": "^3.11"
    },
    "suggest": {
        "sybio/gif-creator": "Install for animated GIF support",
        "sybio/gif-frame-extractor": "Install for animated GIF support"
    },
    "autoload": {
        "psr-4": {
            "Yannickl88\\Image\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yannickl88\\Image\\": "tests/"
        }
    }
}
